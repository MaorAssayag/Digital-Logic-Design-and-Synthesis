`resetall
`timescale 1ns/1ns
module signed_mult_tester (A,
                           B,
                           F
                          );

// Local declarations

parameter M = 3;
parameter N = 3;

output A;
output B;
input  F;

reg [M - 1:0]      A;
reg [N - 1:0]      B;
wire [(M + N) - 1:0] F;
integer i;
integer y;

initial begin 
 	 for (i = 0; i < 2; i = i +1) begin
 	   for (y = 0; y < 2; y = y +1) begin
 	      #5 assign A = i;	  
 	      #5 assign B = y;
        $display("A= %d, B= %d, F= %d", A, B, F);
 	   end
 	 end
 end
endmodule // signed_mult_tester


